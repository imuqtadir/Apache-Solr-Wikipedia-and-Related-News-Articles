|rev6 = [[PopMatters]]
|rev6score = favourable<ref name= "PopMatters"> {{cite web |url=http://www.popmatters.com/pm/review/ironmaiden-dance/ |title=Iron Maiden: ''Dance of Death'' |accessdate=18 June 2011 |publisher=[[PopMatters]] |date=1 October 2003 |last=Begrand |first=Adrien}}</ref>
