|rev4score = favourable<ref name="exclaim">{{cite web |url=http://exclaim.ca/Reviews/Metal/iron_maiden-dance_of_death |title=Iron Maiden  ''Dance of Death'' |publisher=''[[Exclaim!]]'' |first=Sean |last=Palmerston |accessdate=18 September 2012 |date=December 2003}}</ref>
